Cell[BoxData[{
 RowBox[{
  RowBox[{"Log2Factorial", "[", "m_", "]"}], ":=", 
  RowBox[{"Log2", "@", 
   RowBox[{"Factorial", "[", "m", "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"NextPowerOf2", "[", "m_", "]"}], ":=", 
