       RowBox[{"GroupOrbits", "[", "group", "]"}]}], "//", 
      RowBox[{
       RowBox[{"AnyTrue", "[", 
        RowBox[{"#", ",", 
         RowBox[{
          RowBox[{
           RowBox[{"Length", "@", "##"}], ">", "1"}], "&"}]}], "]"}], 
       "&"}]}]}]}], "\[IndentingNewLine]", "]"}]}]}], "Input",
