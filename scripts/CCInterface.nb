      RowBox[{"pp", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"1", "-", "p"}], ",", 
         RowBox[{"p", "/", "3"}], ",", 
         RowBox[{"p", "/", "3"}], ",", 
         RowBox[{"p", "/", "3"}]}], "}"}]}], ",", "\[IndentingNewLine]", 
      "\[Lambda]", ",", "\[Lambda]a", ",", "\[Lambda]m", ",", "\[Lambda]ma"}],
      "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Assert", "[", 
      RowBox[{"kEnv", "\[LessEqual]", "kSys"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Assert", "[", 
      RowBox[{"1", "\[LessEqual]", "kSys", "<", "kTot"}], "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"get", " ", "lambda", " ", "and", " ", "lambda_a"}], " ", 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
