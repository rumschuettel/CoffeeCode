 RowBox[{
  RowBox[{"PauliActionCC", "[", 
   RowBox[{"graph_Graph", ",", "kSys_Integer", ",", "p_", ",", 
    RowBox[{"symmetricSolver_:", "True"}], ",", 
    RowBox[{"extraStabilizedVertices", ":", "_List", ":", 
     RowBox[{"{", "}"}]}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
