Cell["Unique Graph Hash", "Subsection",ExpressionUUID->"e7774731-0488-4da2-aed1-5ab15ce31e1d"],

Cell["\<\
This graph hash can be used in filenames; it canonicalizes the graph first to \
ensure that for different isomorphic graphs one gets the same hash.
Note that, in principle, the filename can be decoded to the underlying graph \
by replacing - with /, Base64-decode, and then ImportString with Graph6 as \
format.\
\>", "Text",ExpressionUUID->"77fe8fbb-5cb2-42fc-941c-025ad873e722"],

Cell[BoxData[{
 RowBox[{"Clear", "[", "GraphHash", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"GraphHash", "[", 
   RowBox[{"g_Graph", ",", "kSys_Integer"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"StringTrim", "@", 
    RowBox[{"ExportString", "[", 
     RowBox[{
      RowBox[{"ExportString", "[", 
       RowBox[{
        RowBox[{"CanonicalizeByOrbits", "[", 
         RowBox[{"g", ",", "kSys"}], "]"}], ",", "\"\<Graph6\>\""}], "]"}], 
      ",", "\"\<Base64\>\""}], "]"}]}], "//", 
   RowBox[{
    RowBox[{"StringReplace", "[", 
     RowBox[{"#", ",", 
      RowBox[{"{", 
       RowBox[{"\"\</\>\"", "\[Rule]", "\"\<-\>\""}], "}"}]}], "]"}], 
    "&"}]}]}]}], "Input",
 InitializationCell->True,
