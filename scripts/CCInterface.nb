            ",", "\"\<JSON\>\""}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"download", "=", 
          RowBox[{"Lookup", "[", 
           RowBox[{
            RowBox[{"First", "@", 
             RowBox[{"Lookup", "[", 
              RowBox[{
               RowBox[{"First", "[", "json", "]"}], ",", "\"\<assets\>\""}], 
              "]"}]}], ",", "\"\<browser_download_url\>\""}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"msg", "=", 
          RowBox[{"\"\<Downloading IGraph/M \>\"", "<>", 
           RowBox[{"Lookup", "[", 
            RowBox[{
             RowBox[{"First", "[", "json", "]"}], ",", "\"\<tag_name\>\""}], 
            "]"}], "<>", "\"\< ...\>\""}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"target", "=", 
          RowBox[{"FileNameJoin", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"CreateDirectory", "[", "]"}], ",", 
             "\"\<IGraphM.paclet\>\""}], "}"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{"$Notebooks", ",", 
           RowBox[{"PrintTemporary", "@", 
            RowBox[{"Labeled", "[", 
