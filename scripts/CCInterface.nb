                    "]"}], "<>", "\"\<>\>\""}], ")"}], "&"}], "/@", "#2"}], 
                ",", "\[IndentingNewLine]", "\"\<\\t>>\>\""}], 
               "\[IndentingNewLine]", "}"}]}], ",", "\"\<\\n\>\""}], "]"}], 
           "&"}], "@@", 
          RowBox[{"{", 
           RowBox[{"idx", ",", "permutationIndices"}], "}"}]}]}], 
        "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}]}], 
      "\[IndentingNewLine]", "}"}]}], "//", "Flatten"}], "//", 
