       "curf", ",", "curG", ",", "\[IndentingNewLine]", "topf", ",", "topG"}],
        "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"curG", "=", "initialG"}], ";", "\[IndentingNewLine]", 
       RowBox[{"curf", "=", 
        RowBox[{"costFunction", "@", "curG"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"topf", "=", "curf"}], ";", "\[IndentingNewLine]", 
