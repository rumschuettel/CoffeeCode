        RowBox[{"{", 
         RowBox[{"\[Lambda]ma", ",", "\[Lambda]a"}], "}"}]}], "}"}], "=", 
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", "\[IndentingNewLine]", 
         RowBox[{"result", "=", 
          RowBox[{"If", "[", 
           RowBox[{"symmetricSolver", ",", "\[IndentingNewLine]", 
            RowBox[{"RunCC", "[", 
