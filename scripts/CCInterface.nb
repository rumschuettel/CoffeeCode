   RowBox[{"group_PermutationGroup", ",", "g_Graph", ",", "kSys_Integer"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"environmentVertices", "=", 
