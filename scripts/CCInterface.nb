  RowBox[{
   RowBox[{"GroupForGraph", "[", 
    RowBox[{"graph", ",", "kSys"}], "]"}], "=", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{"kEnv", "=", 
       RowBox[{
        RowBox[{"VertexCount", "@", "graph"}], "-", "kSys"}]}], 
      "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"PermutationGroup", "[", "\[IndentingNewLine]", 
      RowBox[{"PermutationCycles", "/@", 
       RowBox[{"IGBlissAutomorphismGroup", "[", 
        RowBox[{"{", 
         RowBox[{"graph", ",", 
          RowBox[{"\"\<VertexColors\>\"", "\[Rule]", 
           RowBox[{
            RowBox[{"ConstantArray", "[", 
             RowBox[{"0", ",", "kSys"}], "]"}], "~", "Join", "~", 
            RowBox[{"ConstantArray", "[", 
             RowBox[{"1", ",", "kEnv"}], "]"}]}]}]}], "}"}], "]"}]}], 
      "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
    "]"}]}]}]}], "Input",
 InitializationCell->True,
 CellLabel->"In[3]:=",ExpressionUUID->"75fdcb38-966c-413a-b727-ed354caaa6f9"]
}, Closed]],

