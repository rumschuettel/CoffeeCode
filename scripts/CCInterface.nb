Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"HairGraph", ",", "AllHairGraphs"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"HairGraph", "[", 
   RowBox[{"g_", ",", "hairs_"}], "]"}], ":=", 
  RowBox[{
