         RowBox[{"target", "=", 
          RowBox[{"FileNameJoin", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"CreateDirectory", "[", "]"}], ",", 
             "\"\<IGraphM.paclet\>\""}], "}"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{"$Notebooks", ",", 
           RowBox[{"PrintTemporary", "@", 
            RowBox[{"Labeled", "[", 
             RowBox[{
              RowBox[{"ProgressIndicator", "[", 
               RowBox[{"Appearance", "\[Rule]", "\"\<Necklace\>\""}], "]"}], 
              ",", "msg", ",", "Right"}], "]"}]}], ",", 
           RowBox[{"Print", "[", "msg", "]"}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"URLSave", "[", 
          RowBox[{"download", ",", "target"}], "]"}]}], ",", 
        RowBox[{"Return", "[", "$Failed", "]"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
