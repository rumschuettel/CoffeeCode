            RowBox[{"which", " ", "is", " ", 
             RowBox[{"what", " ", "\[LeftDoubleBracket]", 
              RowBox[{";;", "2"}], "\[RightDoubleBracket]"}], " ", 
             "truncates", " ", 
             RowBox[{"to", ".", "\[IndentingNewLine]", "Therefore"}], " ", 
