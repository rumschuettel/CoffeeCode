      ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"EdgeList", "[", "g", "]"}], "~", "Join", "~", 
       RowBox[{"Thread", "[", 
        RowBox[{"hairs", "\[UndirectedEdge]", "new"}], "]"}]}], "//", 
