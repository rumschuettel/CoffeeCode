Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"updateIGraphM", "[", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"json", ",", "download", ",", "target", ",", "msg"}], "}"}], 
