  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Clear", "[", "switchLeaf", "]"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"switchLeaf", "[", "G_Graph", "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
       "leavesIndices", ",", "\[IndentingNewLine]", "leafIndex", ",", 
