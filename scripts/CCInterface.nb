Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"HairGraph", ",", "AllHairGraphs"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"HairGraph", "[", 
   RowBox[{"g_Graph", ",", "hairs_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"HairGraph", "[", 
    RowBox[{"g", ",", "hairs"}], "]"}], "=", 
