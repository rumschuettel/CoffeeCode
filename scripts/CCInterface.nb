      RowBox[{
       RowBox[{
        RowBox[{"VertexIndex", "[", 
         RowBox[{"g", ",", "#"}], "]"}], "&"}], "/@", 
       RowBox[{"VertexList", "@", 
        RowBox[{"Subgraph", "[", 
         RowBox[{
          RowBox[{"NeighborhoodGraph", "[", 
           RowBox[{"g", ",", "environmentVertices"}], "]"}], ",", 
          RowBox[{"Complement", "[", 
           RowBox[{
            RowBox[{"VertexList", "@", "g"}], ",", "environmentVertices"}], 
           "]"}]}], "]"}]}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
